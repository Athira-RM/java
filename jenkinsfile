pipeline{
	agent any
	
	stages{
		stage ('compile'){
			steps {
				withMaven(maven : 'maven-3.5.4'){
					sh 'mvn clean compile'
				}
				
			}
			
		}
		stage ('test'){
			steps {
				withMaven(maven : 'maven-3.5.4'){
					sh 'mvn test'
				}
			}
		}
		stage('deploy'){
			steps{
				withMaven(maven : 'maven-3.5.4'){
					sh 'mvn deploy'
				}
			}
		}
   }
 }  
			
		
	
